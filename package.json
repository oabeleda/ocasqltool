{
  "name": "OCA Query Tool",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@codemirror/lang-sql": "^6.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@uiw/react-codemirror": "^4.21.8",
    "ag-grid-community": "^29.0.0",
    "ag-grid-react": "^29.0.0",
    "autoprefixer": "^9.8.8",
    "axios": "^1.2.2",
    "craco": "^0.0.3",
    "node-machine-id": "^1.1.12",
    "postcss": "^7.0.39",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-scripts": "4.0.3",
    "sheetjs-style": "^0.15.8",
    "split-pane-react": "^0.1.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
    "web-vitals": "^2.1.4",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.2/xlsx-0.19.2.tgz",
    "xml-js": "^1.6.11"
  },
  "main": "public/main.js",
  "scripts": {
    "start": "export SET NODE_OPTIONS=--openssl-legacy-provider && craco start",
    "build": "export SET NODE_OPTIONS=--openssl-legacy-provider && craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "package-linux": "electron-packager . sqltool --platform=linux --arch=x64 --icon=public/icon-512.png --prune=true --overwrite=true",
    "package-win": "electron-packager . sqltool --platform=win32 --arch=x64 --icon=public/icon-512.ico --prune=true --overwrite=true",
    "generate-license": "node src/license/generator.js",
    "generate-keys": "node src/license/generator.js --generate-keys"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^22.0.0",
    "electron-packager": "^17.1.1",
    "wait-on": "^7.0.1"
  }
}
